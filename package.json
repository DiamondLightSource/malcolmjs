{
  "name": "malcolmjs",
  "config": {
    "reporter": "xunit"
  },
  "scripts": {
    "clean": "rm -rf react-panels",
    "inst:localrpanels": "git clone git@github.com:yousefmoazzam/react-panels.git",
    "inst:rpanels": "npm install react-panels && rm -rf ./node_modules/react-panels/node_modules/react",
    "inst:browserify": "./node_modules/.b in/browserify -t [ babelify --presets [ es2015 react ] ] ./js/app.js > ./bundle.js",
    "install": "npm run inst:localrpanels && npm run inst:rpanels && npm run inst:browserify",
    "test": "jest",
    "compile": "babel --presets es2015 -d lib/ js/",
    "dev": "webpack -d --watch",
    "build": "webpack -p"
  },
  "devDependencies": {
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "expect.js": "^0.3.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jest-cli": "^13.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-coverage": "^1.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-requirejs": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "react-addons-test-utils": "^15.2.0",
    "requirejs": "^2.2.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "browserify": "^13.0.1",
    "flux": "^2.1.1",
    "interact.js": "^1.2.6",
    "less": "^2.7.1",
    "object-assign": "^4.1.0",
    "react": "^15.2.0",
    "react-addons-css-transition-group": "^15.2.0",
    "react-addons-update": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-modal": "^1.4.0",
    "react-sidebar": "^2.1.4",
    "react-toggle": "^2.1.0",
    "react-treeview": "^0.4.5",
    "reactify": "^1.1.1",
    "websocket": "^1.0.23",
    "ws": "^1.1.1"
  },
  "jest": {
    "rootDir": ".",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ],
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "testDirectoryName": "__tests__"
  }
}
